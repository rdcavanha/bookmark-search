{
  "manifest_version": 3,
  "name": "Bookmark Search",
  "version": "1.0.0",
  "description": "Quickly search for bookmarks. Supports tags using `#`.",
  "action": {
    "default_popup": "index.html"
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "64": "icon64.png",
    "128": "icon128.png"
  },
  "permissions": [
    "bookmarks",
    "favicon"
  ],
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Alt+Shift+S",
        "mac": "MacCtrl+Command+S"
      }
    }
  }
}
